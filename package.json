{
  "name": "slacksave-bolt",
  "version": "0.0.1",
  "description": "Save Slack messages using Bolt and Bull.js",
  "main": "main.js",
  "scripts": {
    "start": "run-p watch serve",
    "watch": "parcel watch src/main.js --target node",
    "build": "parcel build src/main.js --target node",
    "serve": "nodemon -w dist dist/main.js",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hieptuanle/slacksave-bolt.git"
  },
  "keywords": [
    "slack",
    "bolt",
    "bullmq"
  ],
  "author": "Hiep Le",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hieptuanle/slacksave-bolt/issues"
  },
  "homepage": "https://github.com/hieptuanle/slacksave-bolt#readme",
  "dependencies": {
    "@slack/bolt": "^2.0.1",
    "bullmq": "^1.8.7",
    "dotenv": "^8.2.0",
    "moment": "^2.27.0",
    "mongodb": "^3.5.6"
  },
  "devDependencies": {
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4"
  }
}
